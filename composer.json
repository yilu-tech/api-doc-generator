{
    "name": "yilu-tech/api-doc-generator",
    "description": "Generate laravel api doc.",
    "authors": [
        {
            "name": "yilu",
            "email": "developer@yilu.co"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "doctrine/annotations": "^1.10"
    },
    "require-dev": {
        "orchestra/testbench": "^5.3"
    },
    "autoload": {
        "classmap": [
        ],
        "psr-4": {
            "YiluTech\\ApiDocGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YiluTech\\ApiDocGenerator\\ApiDocGeneratorServiceProvider"
            ],
            "aliases": {
            }
        }
    },
    "scripts": {},
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "secure-http": false
    }
}
